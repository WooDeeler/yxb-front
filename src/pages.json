    {
      "path": "pages/postPage/index",
      "style": {
        "navigationBarTitleText": "发帖"
      }
    },
    {
      "path": "pages/message/index",
      "style": {
        "navigationBarTitleText": "消息"
      }
    },
    {
      "path": "pages/lib/index",
      "style": {
        "navigationBarTitleText": "资源库"
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
