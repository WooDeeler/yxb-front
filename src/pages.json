{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "研习宝"
      }
    },
    {
      "path": "pages/searchPage/index",
      "style": {
        "navigationBarTitleText": "搜索"
      }
    },
    {
      "path": "pages/searchResult/index",
      "style": {
        "navigationBarTitleText": "搜索结果"
      }
